      "targets": [
        {
          "dataset": "MediaReviewsDB",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH company_genre_scores AS (\r\n    SELECT \r\n        c.company_name AS Company,\r\n        g.genre_title AS Genre,\r\n        SUM(fr.rating * bmg.weight) AS weighted_score\r\n    FROM \"FACT_REVIEWS\" as fr\r\n    JOIN \"DIM_MEDIA_INFO\" as mi ON fr.media_info_id = mi.id\r\n    JOIN \"BRIDGE_MEDIA_GENRE\" as bmg ON mi.id = bmg.media_id\r\n    JOIN \"GENRES\" as g ON bmg.genre_id = g.id\r\n    JOIN \"BRIDGE_MEDIA_COMPANY\" as bmc ON mi.id = bmc.media_id\r\n    JOIN \"COMPANIES\" as c ON bmc.company_id = c.id\r\n    GROUP BY c.company_name, g.genre_title\r\n)\r\nSELECT company as Company, genre as Best_Rated_Genre\r\nFROM (\r\n    SELECT *, \r\n    ROW_NUMBER() OVER (PARTITION BY company ORDER BY weighted_score DESC) as rn\r\n    FROM company_genre_scores\r\n) t\r\nWHERE rn = 1;",
          "refId": "A",
          "sql": {
            "columns": [
