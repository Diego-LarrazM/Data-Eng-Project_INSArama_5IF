# -- Network --------------------------------
INSARAMA_NET=insarama_net

# -- Postgres Configuration (Star Schema) --------------------------------
DW_POSTGRES_HOST=insm_postgres_data_warehouse  # ATTENTION If changed change in dockercompose too
DW_POSTGRES_DB=MediaReviewsDB

DW_POSTGRES_USER=insarama
DW_POSTGRES_PASSWORD=insarama

DW_POSTGRES_LOAD_BATCH_SIZE=1000 # How many documents are loaded from Mongo and batch-persisted to Postgres

DW_POSTGRES_COM_PORT=8080 # port we connect to postgres 5432 port

# -- Extractor Configuration --------------------------------
IMDB_FILES_TO_DOWNLOAD=name.basics #name.basics title.akas title.basics title.crew title.principals
IMDB_DATA_URL=https://datasets.imdbws.com/
DATA_FILE_DIRECTORY=/app/data # Docker container local dir from root

# -- MongoDB Transient Storage Configuration --------------------------------
MONGO_USERNAME=insarama
MONGO_PASSWORD=insarama
MONGO_HOST_NAME=insm_mongo_transient_serv
MONGO_RSET_NAME=insm_mongo_transient_replica
MONGO_PORT=27017
MONGO_HEALTHCHECK_RETRIES=3

MONGO_DB=insarama_db
MONGO_MEDIA_COLLECTION=media_collection
